    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.49.9"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
